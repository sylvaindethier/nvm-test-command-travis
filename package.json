{
  "name": "nvm-test-command-travis",
  "version": "0.0.0",
  "description": "Travis command for `nvm-test`",
  "main": "dist/index.js",
  "scripts": {
    "-start": "NODE_ENV=development babel-node ./src/bin.js",
    "clean": "rimraf dist",
    "lint": "eslint ./",
    "prebuild": "npm run clean && npm run lint",
    "build": "npm run build:dist",
    "build:dist": "NODE_ENV=production babel ./src -d ./dist",
    "test": "npm run coverage",
    "test:only": "mocha --compilers js:babel-core/register --recursive",
    "coverage": "rimraf coverage && istanbul cover _mocha -- -R spec --compilers js:babel-core/register --recursive",
    "preversion": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sylvaindethier/nvm-test-command-travis.git"
  },
  "keywords": [
    "travis",
    "nvm-test",
    "nvm",
    "test"
  ],
  "author": "Sylvain Dethier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylvaindethier/nvm-test-command-travis/issues"
  },
  "homepage": "https://github.com/sylvaindethier/nvm-test-command-travis#readme",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
  },
  "peerDependencies": {
    "nvm-test": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0-beta",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "expect": "^1.15.2",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  }
}
